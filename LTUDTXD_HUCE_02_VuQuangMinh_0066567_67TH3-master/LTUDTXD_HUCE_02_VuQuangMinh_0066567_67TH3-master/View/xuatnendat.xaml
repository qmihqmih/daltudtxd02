<Window x:Class="LTUDTXD_HUCE_2_VuQuangMinh_0066567_67TH3.View.xuatnendat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LTUDTXD_HUCE_2_VuQuangMinh_0066567_67TH3.View"
        mc:Ignorable="d"
        Title="xuatnendat" Height="200" Width="400">
    <Grid Background="LightBlue" >
        <StackPanel Margin="30" HorizontalAlignment="Center" VerticalAlignment="Center" >

            <!-- Tiêu đề -->
            <TextBlock Text="Báo Cáo Nền Đất" FontSize="26" FontWeight="Bold" Margin="0,0,0,30"
                   HorizontalAlignment="Center" Foreground="#2E86C1"/>


            <WrapPanel Orientation="Horizontal" HorizontalAlignment="Center" ItemWidth="150" ItemHeight="60">
                <Button Content="Xuất" Margin="10" Padding="10" Command="{Binding ExportToExcelCommand}"/>



            </WrapPanel>

        </StackPanel>
    </Grid>
</Window>
